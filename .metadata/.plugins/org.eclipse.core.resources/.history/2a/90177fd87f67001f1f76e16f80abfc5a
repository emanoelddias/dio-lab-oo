package set;

public class TesteConvidados {
	
	public static void main(String[] args) {
		ConjuntoConvidados lista1 = new ConjuntoConvidados();
		
		lista1.adicionarConvidado("nome1", 1231);
		lista1.adicionarConvidado("nome2", 1232);
		lista1.adicionarConvidado("nome3", 1233);
		lista1.adicionarConvidado("nome4", 1234);
		lista1.adicionarConvidado("nome5", 1235);
		lista1.adicionarConvidado("nome6", 1236);
		lista1.adicionarConvidado("nome7", 1236);
		
		lista1.contarConvidados();
		//System.out.println(lista1.exibirConvidados());
		
		lista1.removerConvidadoPorCodigoConvite(1233);
		lista1.contarConvidados();
		System.out.println(lista1.exibirConvidados());
		
	}

}
