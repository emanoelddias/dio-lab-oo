package set;

public class TestePalavrasUnicas {
	
	public static void main(String[] args) {
		
		ConjuntoPalavrasUnicas listaPalavrasUnicas = new ConjuntoPalavrasUnicas();
		
		listaPalavrasUnicas.adicionarPalavra("Palavra1");
		listaPalavrasUnicas.adicionarPalavra("Palavra2");
		listaPalavrasUnicas.adicionarPalavra("Palavra3");
		listaPalavrasUnicas.adicionarPalavra("Palavra4");
		listaPalavrasUnicas.adicionarPalavra("Palavra5");
		listaPalavrasUnicas.adicionarPalavra("Palavra6");
		listaPalavrasUnicas.adicionarPalavra("Palavra7");
		
		listaPalavrasUnicas.quantidadePalavras();
		
		listaPalavrasUnicas.removerPalavra("Palavra5");
		
		listaPalavrasUnicas.quantidadePalavras();
		
		System.out.println(listaPalavrasUnicas.verificarPalavra("Palavra3"));
		
		listaPalavrasUnicas.removerPalavra("Palavra3");
		
		System.out.println(listaPalavrasUnicas.verificarPalavra("Palavra3"));
	}
}
