package set.ordenacao;

public class TesteAlunos {
	
	public static void main(String[] args) {
		GerenciadorAlunos listaAlunos = new GerenciadorAlunos();
		
		listaAlunos.adicionarAluno("Nome1", 123456, 5.5);
		listaAlunos.adicionarAluno("Nome2", 123458, 2.5);
		listaAlunos.adicionarAluno("Nome3", 123455, 3.5);
		listaAlunos.adicionarAluno("Nome6", 123457, 5.5);
		listaAlunos.adicionarAluno("Nome8", 123451, 8.5);
		listaAlunos.adicionarAluno("Nome4", 132456, 9.5);
		listaAlunos.adicionarAluno("Nome9", 126456, 1.5);
		listaAlunos.adicionarAluno("Nome0", 123456, 2.0);
		
		//System.out.println(listaAlunos.exibirAlunosPorNome());
		//System.out.println(listaAlunos.exibirAlunosPorNota());
		System.out.println(listaAlunos.exibirAlunos());
		
		listaAlunos.removerAluno(132456);
		
		System.out.println(listaAlunos.exibirAlunos());		
	}

}
